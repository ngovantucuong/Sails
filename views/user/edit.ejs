<form action="/user/update/<%= user.id %>" method='post' class="form-signin">
	<h2> Hey, you're editing a user...</h2>

	<input value="<%= user.name %>" name="name" type="text" class="form-control"/>
	<input value="<%= user.title %>" name="title" type="text" class="form-control"/>
	<input value="<%= user.email %>" name="email" type="email" id="button" class="form-control"/>
	<% if (session.authenticated && session.User.admin) { %>
			<% if (user.admin) { %>
				<input type="hidden" name="administrator" id='administrator' value="unchecked">
				<label class="checkbox"><input type="checkbox" id='box' name="box" checked onclick="validate()"/> Admin</label>
			<% } else { %>
				<input type="hidden" name="administrator" id='administrator' value="unchecked">
				<label class="checkbox"><input type="checkbox" id='box' name="box" onclick="validate()"/> Admin</label>
			<% } %> 
		<% } %> 
	<input value="Process" type="submit" class="btn btn-lg btn-primary btn-block"/>
	<input name="_csrf" type="hidden" value="<%= _csrf %>" />
</form>	